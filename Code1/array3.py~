print("\n\n Welcome to Array \n\n")
array1 = [
        [int(input("a1[0][0] = ")),int(input("a1[0][1] = ")),int(input("a1[0][2] = ")),int(input("a1[0][3] = "))],
        [int(input("a1[1][0] = ")),int(input("a1[1][1] = ")),int(input("a1[1][2] = ")),int(input("a1[1][3] = "))],
        [int(input("a1[2][0] = ")),int(input("a1[2][1] = ")),int(input("a1[2][2] = ")),int(input("a1[2][3] = "))],
        [int(input("a1[3][0] = ")),int(input("a1[3][1] = ")),int(input("a1[3][2] = ")),int(input("a1[3][3] = "))],
        [int(input("a1[4][0] = ")),int(input("a1[4][1] = ")),int(input("a1[4][2] = ")),int(input("a1[4][3] = "))],
        ]

array2 = [
        [int(input("a2[0][0] = ")),int(input("a2[0][1] = ")),int(input("a2[0][2] = ")),int(input("a1[0][3] = "))],
        [int(input("a2[1][0] = ")),int(input("a2[1][1] = ")),int(input("a2[1][2] = ")),int(input("a1[1][3] = "))],
        [int(input("a2[2][0] = ")),int(input("a2[2][1] = " )),int(input("a2[2][2] = ")),int(input("a1[2][3] = "))],
        [int(input("a2[3][0] = ")),int(input("a2[3][1] = ")),int(input("a2[3][2] = ")),int(input("a1[3][3] = "))],
        [int(input("a2[4][0] = ")),int(input("a2[4][1] = ")),int(input("a2[4][2] = ")),int(input("a1[4][3] = "))],
        ]


print("Given Array 1 = ")

for row in range(5):

    for col in range(4):

        print(array1[row][col],end='     ')
    print()

print("Given Array 2 = ")

for row in range(5):

    for col in range(4):

        print(array2[row][col],end='     ')
    print()

print("printing Addition of Array ")

for row in range(5):

    for col in range(4):


        print(array1[row][col]+array2[row][col], end = '     ')

    print()



print("\n\n")
